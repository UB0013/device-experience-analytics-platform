{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": true,
  "panels": [
    {
      "title": "Events Produced / sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(telemetry_events_produced_total[1m])",
          "legendFormat": "{{topic}} - {{event_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "title": "Current Throughput",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "telemetry_events_per_second",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "events/s",
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 500 },
              { "color": "green", "value": 1000 }
            ]
          }
        }
      }
    },
    {
      "title": "Produce Errors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "rate(telemetry_produce_errors_total[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "errors/s",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "title": "Produce Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(telemetry_produce_latency_seconds_bucket[5m]))",
          "legendFormat": "p95 latency",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(telemetry_produce_latency_seconds_bucket[5m]))",
          "legendFormat": "p50 latency",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Events by Topic",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum by (topic) (telemetry_events_produced_total)",
          "legendFormat": "{{topic}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "API Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "rate(api_requests_total[1m])",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "API Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(api_request_latency_seconds_bucket[5m]))",
          "legendFormat": "{{endpoint}} p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    }
  ],
  "refresh": "5s",
  "schemaVersion": 39,
  "tags": ["telemetry", "platform"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "utc",
  "title": "Telemetry Platform Overview",
  "uid": "telemetry-overview"
}
